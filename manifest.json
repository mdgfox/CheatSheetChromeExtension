{
	"manifest_version": 2,
	"name": "Cheat Sheet Extension",
	"description": "Маленький помошник в прохождении тестов в браузере",
	"version": "1.0.0",
  "browser_action": {
		"default_icon": "icon_128.png",
		"default_popup": "index.html"
	},
    "icons": {"128": "icon_128.png"},
	"permissions": ["activeTab"],
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["index.js"]
        }
      ]
}