{
    "name": "Cheat Sheet Extension",
    "description": "Simple google chrome extension to solve test.",
    "version": "1.0.7",
    "manifest_version": 3,
    "background": {
        "service_worker": "init.js",
        "type": "module"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "index.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "databases/default.json"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/icons/16.png",
            "32": "/icons/32.png",
            "48": "/icons/48.png",
            "128": "/icons/128.png"
        }
    },
    "icons": {
        "16": "/icons/16.png",
        "32": "/icons/32.png",
        "48": "/icons/48.png",
        "128": "/icons/128.png"
    }
}
